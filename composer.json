{
    "name": "apantle/laravel-notification-apn-http2",
    "description": "Dependency free Apple Push Notifications provider, configurable for multiple apps",
    "keywords": [
        "apantle",
        "notifications",
        "apple-push-notification"
    ],
    "homepage": "https://github.com/tzkmx/http2-apple-laravel-notification",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Jesus Franco",
            "email": "jefrancomix@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "^6.0 | ^7.0",
        "illuminate/notifications": "^6.0 | ^7.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Apantle\\ApnHttp2Notification\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Apantle\\ApnHttp2Notification\\ApnHttp2ServiceProvider"
            ]
        }
    }
}
